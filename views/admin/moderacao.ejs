<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ADMIN CAFEXPRESS</title>
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <%- include("../partials/header") %>
    <main>
        <section class="contato">
            <div class="container">
                <h1>Moderação de usuários</h1>
                <div class="align-row" style="width: 100%;">
                  <table>
                    <tr>
                      <th>ID</th>
                      <th>Nome</th>
                      <th>Email</th>
                      <th>categoria</th>
                      <th>Ações</th>
                    </tr>
                    <% usuarios.forEach(function(usuario) { %>
                    <tr>
                      <td><%= usuario.id %></td>
                      <td><%= usuario.nome %></td>
                      <td><%= usuario.email %></td>
                      <td>R$ <%= usuario.categoria %></td>
                      <td>
                        <a href="">Editar</a>
                        <a href="">Remover</a>
                      </td>
                    </tr>
                    <% }) %>
                  </table>
                </div>
            </div>
        </section>
    </main>
    <%- include("../partials/footer") %>
</body>

</html>